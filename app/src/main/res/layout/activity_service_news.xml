<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#00000000"
    android:fitsSystemWindows="true"
    android:clipToPadding="true"
    tools:context=".View.Activity.ServiceNewsCenterActivity">
    <include layout="@layout/tool_bar"/>
    <com.awarmisland.android.popularrefreshlayout.RefreshLayout
        android:id="@+id/refresh"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/y6"
            android:background="#fff">
            <com.smartcityin.waterknow.Base.PushListView
                android:id="@+id/service_newsCenter_lv"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:dividerHeight="@dimen/y28"
                android:divider="#fff"
                android:scrollbars="none"></com.smartcityin.waterknow.Base.PushListView>
        </RelativeLayout>
    </com.awarmisland.android.popularrefreshlayout.RefreshLayout>
</LinearLayout>